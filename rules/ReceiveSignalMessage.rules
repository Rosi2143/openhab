rule "Signal message changed"
when
    Item signal_source received command
then       
    val signal_message      = signal_message as GenericItem
    val signal_source       = signal_source as GenericItem

    var s-msg = signal_message.state
    var s-source = signal_source.state

    executeCommandLine("/etc/openhab/send-signal.sh","+41792279112 ","habe eine meldung bekommen"+s-msg+"xxx"+s-source)
end