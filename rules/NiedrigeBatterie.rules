rule "Temperatursensor1_NiedrigerBatteriestatus"
when
    Item Temperatursensor1_NiedrigerBatteriestatus changed from NULL to ON
then       
    executeCommandLine("/etc/openhab/send-signal.sh","+41792279112 ","Temperatursensor1_NiedrigerBatteriestatus")
end

